{
    "name": "slince/config",
    "description": "A flexible configuration file loader that supports multiple file formats",
    "keywords": ["configuration", "config", "json", "yaml", "yml", "ini", "xml", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tao",
            "email": "taosikai@yeah.net"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Slince\\Config\\": "./"
        }
    },
    "suggest": {
        "openlss/lib-array2xml": "requires for parsing xml file",
        "symfony/yaml": "Requires for parsing yaml file"
    }
}
